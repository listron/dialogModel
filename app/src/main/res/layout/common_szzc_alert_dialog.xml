    <!-- onetitle_onebutton 样式 -->

    <RelativeLayout
        android:id="@+id/onetitle_onebutton"
        android:layout_width="@dimen/dd_dimen_490px"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@mipmap/save_dialog_bg"
        android:visibility="gone" >

        <TextView
            android:id="@+id/onetitle_onebutton_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dd_dimen_40px"
            android:layout_marginTop="@dimen/dd_dimen_60px"
            android:gravity="center_horizontal"
            android:paddingLeft="@dimen/dd_dimen_26px"
            android:paddingRight="@dimen/dd_dimen_26px"
            android:textColor="@color/dialog_text_default"
            android:textSize="@dimen/dd_dimen_28px" />

        <LinearLayout
