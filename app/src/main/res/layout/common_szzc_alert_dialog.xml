            android:layout_height="@dimen/dd_dimen_110px"
            android:layout_below="@id/onetitle_onebutton_title"
            android:layout_centerHorizontal="true"
            android:paddingBottom="@dimen/dd_dimen_23px" >

            <Button
                android:id="@+id/onetitle_onebutton_button"
                android:layout_width="@dimen/dd_dimen_440px"
                android:layout_height="@dimen/dd_dimen_80px"
                android:background="@drawable/yellow_button_selector"
                android:textColor="@color/white"
                android:textSize="@dimen/dd_dimen_28px" />
        </LinearLayout>
    </RelativeLayout>

